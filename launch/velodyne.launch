	<arg name="rpm" value="1200" />
    </include>

    <node pkg="tf" type="static_transform_publisher" name="velodyne_tf"
        args="0 -0.1 0 0 -1.570796 1.570796 jetson6_ir_optical_frame2 velodyne 100"/>

